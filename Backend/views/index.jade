extends ../public/_bootstrap

append styles
    +bootswatch("united")
    link(href='./stylesheets/theme.css', rel='stylesheet')
    title=title

block body
    +navbar-fixed("Lupapistos")

    if data.length > 0
        ul
            each hearing in data
                li= hearing.hearee.firstName + ' ' + hearing.hearee.lastName + ', ' + hearing.hearee.address